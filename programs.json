[
    {"name": "assetfinder", "cmd": "assetfinder $TARGET | tee $TARGET_assetfinder.txt"},
    {"name": "Subfinder -d", "cmd": "subfinder -d $TARGET | tee $TARGET_subfinder.txt"},
    {"name": "Subfinder -list", "cmd": "subfinder -l $TARGET_FILE | tee $TARGET_subfinder_list.txt"},
    {"name": "Sublist3r", "cmd": "sublist3r -d $TARGET -b -p 21,22,25,80,443,445,8080 -v -t 100 -e google.com | tee $TARGET_sublist3r.txt"},
    {"name": "Amass", "cmd": "amass enum -d $TARGET -o $TARGET_amass.txt"},
    {"name": "MassDNS", "cmd": "massdns -r ~/tools/multiviz/resolve.txt -t A -o S -w /usr/share/seclists/Discovery/DNS/bug-bounty-program-subdomains-trickest-inventory.txt $TARGET_massdns.txt"},
    {"name": "Sort and Unique", "cmd": "cat $TARGET_assetfinder.txt $TARGET_amass.txt $TARGET_subfinder.txt $TARGET_amass.txt | sort | uniq | tee $TARGET_unique_doms.txt"},
    {"name": "Resolver", "cmd": "shuffledns -list $TARGET_unique_doms.txt -r ~/tools/multiviz/resolve.txt -mode resolve | tee $TARGET_resolved.txt"},
    {"name": "Dnsx recon", "cmd": "dnsx -d $TARGET -cdn -recon -re -w /usr/share/seclists/Discovery/DNS/bug-bounty-program-subdomains-trickest-inventory.txt -r ~/tools/multiviz/resolve.txt"},
    {"name": "HTTprobe", "cmd": "cat $TARGET_resolved.txt | httprobe | tee $TARGET_httprobed.txt"},
    {"name": "403-checker", "cmd": "sudo ~/tools/multiviz/curly.sh $TARGET_unique_doms.txt ~/tools/multiviz/403.txt | tee $TARGET_403.txt"},
    {"name": "Waybackpy", "cmd": "waybackpy -u https://$TARGET -ku -sub | tee $TARGET_wayback_urls.txt"},
    {"name": "Gau", "cmd": "echo $TARGET | sudo gau | tee $TARGET_gau.txt"},
    {"name": "Sort URLs", "cmd": "cat $TARGET_httprobed.txt $TARGET_waybackurls.txt $TARGET_gau.txt | sort | uniq | tee $TARGET_unique_URLs.txt"},
    {"name": "Nuclei", "cmd": "nuclei -l $TARGET_unique_doms.txt -t /home/kali/.bbot/tools/nuclei-templates/ -o $TARGET_nuclei.txt"},
    {"name": "GF Pattern Matching", "cmd": "gf xss $TARGET_unique_URLs.txt | tee $TARGET_gf_xss.txt"},
    {"name": "Sn1per", "cmd": "sudo sniper -f $TARGET_unique_doms.txt -m nuke -w $TARGET_Workspace | tee $TARGET_sniper.txt"},
    {"name": "Autorecon", "cmd": "sudo autorecon -t $TARGET_unique_doms.txt --all-plugins --accessible -v --wpscan.api-token Z8IRzWJRsn0CbxrZFNy8vvfAzff6xBunqbMY8kmhu3s | tee $TARGET_autorecon.txt"},
    {"name": "LinkFinder", "cmd": "python3 /usr/share/sniper/plugins/LinkFinder/linkfinder.py -d 1000 -v $TARGET_unique_URLs.txt | tee $TARGET_links_found.txt"},
    {"name": "Meg", "cmd": "meg -d 1000 -v / $TARGET_httprobed.txt | tee $TARGET_MEG_massout.txt"},
    {"name": "FFUF", "cmd": "ffuf -w ~/wordlists/raft-large-directories.txt -u https://$TARGET/FUZZ -o $TARGET_ffuf.txt"},
    {"name": "SQLmap", "cmd": "sqlmap -m $TARGET_httprobed.txt --batch --output-dir=$TARGET_sqlmap"},
    {"name": "XSStrike", "cmd": "python3 /home/kali/tools/XSStrike/xsstrike.py -u $TARGET --blind --file /usr/share/seclists/Fuzzing/XSS/robot-friendly/XSS-Cheat-Sheet-PortSwigger.txt -e base64 --crawl --headers ~/tools/multiviz/403.txt -l 4 -o $TARGET_xsstrike.json"},
    {"name": "Dirsearch", "cmd": "dirsearch -u https://$TARGET -e php,html,js,txt -x 400,403,404 -w /usr/share/wordlists/dirbuster/directory-list-2.3-medium.txt -t 50 -o $TARGET_dirsearch.txt"},
    {"name": "Subjack", "cmd": "subjack -w $TARGET_unique_doms.txt -t 100 -timeout 30 -ssl -v 3 -c ~/go/src/github.com/haccer/subjack/fingerprints.json -o $TARGET_subjack.txt"},
    {"name": "CSP Evaluator", "cmd": "python3 ~/tools/csp_evaluator/evaluate.py --domains $TARGET_unique_doms.txt --output $TARGET_csp.txt"},
    {"name": "GitHound", "cmd": "githound --dig-files --dig-commits --many-results --repo-url https://github.com/$TARGET --output $TARGET_githound.txt"},
    {"name": "LinkFinder", "cmd": "python3 ~/tools/LinkFinder/linkfinder.py -i $TARGET -o cli | tee $TARGET_linkfinder.txt"}
]
